#include <stdio.h>
//Escreva um programa que calcule o fatorial de um nÃºmero usando um loop while.

int main(){
    int numero;
    int fatorial = 1;
    
    scanf("%d", &numero);

    while (numero >= 1)
    {
        fatorial *=numero;
        numero--;
    }
    
    printf("O fatorial = %.d\n", fatorial);

}