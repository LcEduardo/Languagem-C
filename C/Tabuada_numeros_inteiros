//Tabuada de todos os números inteiros de 1 até 10.

#include <stdio.h>
#include <locale.h>

int main (){
	
	setlocale(LC_ALL,"Portuguese");
	
	printf("-----TABUADA DOS NÚMEROS DE 1 A 10-----\n");
	
	int i, j;
	int resultado = 0;
	
	for(i = 1; i <= 10; i++){

		for(j = 1; j <= 10; j++){
			resultado = i + j;
			printf("%d + %d = %d\n",i, j, resultado);
		}
        
		printf("\n\n");
	}
}